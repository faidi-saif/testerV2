{
    "description": "CO : dual not cropped ",
    "steps":
    [
      {
        "case": "still",
        "params":
        {
          "shooting_mode"  : "dual",
          "params_mode"    :
           {
             "fps"            : "30",
             "submode"        : "CAL"
           },
          "options_mode" :
          {
            "flare"         : "0",
            "flare_art"     : "",
            "bypass"        : "enable",
            "ring_low_res"  : "disable",
            "dump_raw"      : "disable",
            "raw_nbits"     : "16",
            "bayer_width"   : "4056",
            "mpx"           : "12MP",
            "debug_dump"    : "disable"

          }
        },
        "logs" : "~/Desktop/test_logs/C0/serials"
      },
            {
      "case": "reset",
      "params":
      {
        "option": "soft"
      }
      },
      {
        "case": "checker",
        "params":
        {
          "out_directory"  : "~/Desktop/test_logs/C0/photos",
          "TypeChecker"    : "PhotoStat"
        }
      }

    ]
}
